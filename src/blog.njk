{% extends "base.njk" %}
{% block postContent %}
  <ul>
  {% for post in collections.posts %}
    <li>
      <strong>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
      </strong>
      -
      <time datetime="{{ post.date.toString() }}">{{ post.date.toDateString() }}</time>
      -
      <span>{{ post.data.description }}</span>
    </li>
  {% endfor %}
</ul>
{% endblock %}
