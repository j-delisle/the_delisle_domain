{
  "name": "the_delisle_domain",
  "version": "1.0.0",
  "description": "My Personal blog website",
  "main": ".eleventy.js",
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "dotenv": "^8.2.0",
    "netlify-cms": "^2.10.48",
    "node-fetch": "^2.6.1",
    "postcss-clean": "^1.1.0",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-css-variables": "^0.17.0",
    "postcss-nesting": "^7.0.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^6.7.0"
  },
  "scripts": {
    "build_11ty": "eleventy",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "clean": "rm -rf _site",
    "dev": "npm-run-all clean --parallel dev:*",
    "dev:postcss": "postcss src/static/css/main.css -d _site/static/css --config ./postcss.config.js -w",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --watch --quiet",
    "dev:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode",
    "testProd:postcss": "postcss src/static/css/main.css -d _site/static/css --config ./postcss.config.js -w",
    "testProd:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --watch --quiet",
    "testProd:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode",
    "testbuild": "NODE_ENV=production npm-run-all clean --parallel testProd:*",
    "prod:postcss": "postcss src/static/css/main.css -d _site/static/css --config ./postcss.config.js",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*"
  },
  "keywords": [],
  "author": "Jay Delisle",
  "license": "ISC"
}
